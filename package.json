    "lint-staged": {
        "*.{js,jsx,vue,ts,tsx}": [
            "vue-cli-service lint",
            "git add"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
